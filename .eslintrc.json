{
	"extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
	"plugins": ["react", "react-hooks"],
	"rules": {
		"react/jsx-filename-extension": [
			"error",
			{
				"extensions": [".tsx"]
			}
		],
		"react-hooks/rules-of-hooks": "error",
		"react-hooks/exhaustive-deps": "warn"
	},
	"settings": {}
}
